{
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "plugins": [
        "prettier-plugin-multiline-arrays"
    ],
    "overrides": [
        {
            "files": [
                "*.json"
            ],
            "options": {
                "multilineArraysWrapThreshold": 0
            }
        }
    ]
}
